		<thead><tr><th>Function name</th><th>Type</th><th width="240px">Params</th><th>Description</th></tr></thead>
		<tbody>
		<tr><td><b>flattenArrays</b></td><td>Function&lt;T[][],T[]&gt;</td><td></td><td></td></tr>
		</tbody></table>
        <!--  -->
        </div>
        
      </subsection>
      
